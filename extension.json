{
  "manifest_version": 2,
  "name": "MWUnit",
  "namemsg": "mwunit-extensionname",
  "type": "other",
  "author": [
    "[https://www.mediawiki.org/wiki/User:Xxmarijnw Marijn van Wezel]"
  ],
  "version": "0.8",
  "url": "https://www.mediawiki.org/wiki/Extension:MWUnit",
  "description": "A unit testing framework for MediaWiki",
  "descriptionmsg": "mwunit-description",
  "license-name": "MIT",
  "requires": {
    "MediaWiki": ">= 1.31.0"
  },
  "MessagesDirs": {
    "MWUnit": [
      "i18n"
    ]
  },
  "ExtensionMessagesFiles": {
    "MWUnitMagic": "i18n/MWUnitMagic.i18n.php"
  },
  "AutoloadNamespaces": {
    "MWUnit\\": "src/",
    "MWUnit\\Assertion": "src/Assertion/",
    "MWUnit\\Exception": "src/Exception/",
    "MWUnit\\Specials": "src/Specials/",
    "MWUnit\\Controllers": "src/Controllers/"
  },
  "Hooks": {
    "ParserFirstCallInit": "MWUnit\\MWUnit::onParserFirstCallInit",
    "LoadExtensionSchemaUpdates": "MWUnit\\MWUnit::onLoadExtensionSchemaUpdates",
    "PageContentSaveComplete": "MWUnit\\UpdateHandler::onPageContentSaveComplete",
    "PageContentInsertComplete": "MWUnit\\UpdateHandler::onPageContentInsertComplete",
    "SkinBuildSidebar": "MWUnit\\MWUnit::onSkinBuildSidebar",
    "ParserFetchTemplate": "MWUnit\\TestCaseRun::onParserFetchTemplate"
  },
  "SpecialPages": {
    "MWUnit": "MWUnit\\Specials\\SpecialMWUnit"
  },
  "config": {
    "MWUnitMaxTestExecutionTime": {
      "value": 600,
      "descriptionmsg": "mwunit-config-max-text-execution-time"
    },
    "MWUnitDefaultTestDox": {
      "value": false,
      "descriptionmsg": "mwunit-config-default-testdox"
    },
    "MWUnitBackupGlobals": {
      "value": true,
      "descriptionmsg": "mwunit-config-backup-globals"
    },
    "MWUnitStrictCoverage": {
      "value": true,
      "descriptionmsg": "mwunit-config-strict-coverage"
    },
    "MWUnitForceCoversAnnotation": {
      "value": false,
      "descriptionmsg": "mwunit-config-force-covers-annotation"
    }
  },
  "namespaces": [
    {
      "id": 5750,
      "constant": "NS_TEST",
      "name": "Test"
    },
    {
      "id": 5751,
      "constant": "NS_TEST_TALK",
      "name": "Test_talk"
    }
  ],
  "AvailableRights": [
    "mwunit-runtests"
  ]
}